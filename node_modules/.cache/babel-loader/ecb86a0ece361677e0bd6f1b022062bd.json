() {\n      this.handleReset();\n      this.form = this.data;\n    },\n    handleReset() {\n      this.$refs.form.resetFields();\n    },\n    handleSubmit() {\n      this.$refs.form.validate(valid => {\n        if (valid) {\n          // this.postRequest(\"请求路径\", this.form).then(res => {\n          //   this.submitLoading = false;\n          //   if (res.success) {\n          //     this.$Message.success(\"编辑成功\");\n          //     this.submited();\n          //   }\n          // });\n          // 模拟成功\n          this.submitLoading = false;\n          this.$Message.success(\"编辑成功\");\n          this.submited();\n        }\n      });\n    },\n    close() {\n      this.$emit(\"close\", true);\n    },\n    submited() {\n      this.$emit(\"submited\", true);\n    }\n  },\n  mounted() {\n    this.init();\n  }\n};\n</script>"],"sourceRoot":"src/views/xboot-vue-template/single-window"}]}